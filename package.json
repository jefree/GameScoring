{
  "name": "game-scoring",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "js": "mkdirp dist/js && watchify -o dist/js/bundle.js -v -d src/js/app.js -t [ babelify --presets [ es2015 react stage-0] ]",
    "css": "mkdirp dist/css && catw -c 'node-sass' src/css/*.scss -o dist/css/style.css -v",
    "web": "live-server --watch=dist",
    "start": "npm run web & npm run css & npm run js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "catw": "^1.0.1",
    "live-server": "^1.2.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.5.0",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.5",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.5.2",
    "react-addons-transition-group": "^15.5.2",
    "react-dom": "^15.4.2",
    "reactstrap": "^4.5.0"
  }
}
